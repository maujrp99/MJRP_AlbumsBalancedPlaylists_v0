{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "mjrp-proxy",
          "region": "southamerica-east1"
        }
      },
      {
        "source": "**",
        "destination": "/index-v2.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}